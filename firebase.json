{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/index",
        "destination": "/index.html"
      },
      {
        "source": "/auth/google",
        "destination": "/auth/google"
      }
    ]
  }
}
